      #include<bits/stdc++.h>
using namespace std;

int main()
{
  string txt = "abababcd";
  cout<<txt<<endl;
  string pat = "abab";
int n = txt.length();
int m = pat.length();
int j ;
  for(int i = 0 ; i < n-m+1; i ++)
  {
    for(j = 0; j < m;j++ )
    {
      if(txt[i+j] != pat[j])
        break;
      
    }if(j == m) cout<< i<<endl;
  }
  return 0;
}
